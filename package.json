{
	"name": "@natoboram/heroicons.svelte",
	"version": "0.0.1",
	"description": "Exports Heroicons for usage with SvelteKit.",
	"keywords": [
		"Heroicons",
		"SvelteKit",
		"TailwindCSS"
	],
	"homepage": "https://github.com/NatoBoram/heroicons.svelte",
	"bugs": {
		"url": "https://github.com/NatoBoram/heroicons.svelte/issues"
	},
	"license": "LGPL-3.0-or-later",
	"author": {
		"name": "Nato Boram",
		"url": "https://github.com/NatoBoram"
	},
	"funding": [
		{
			"type": "patreon",
			"url": "https://www.patreon.com/NatoBoram"
		},
		{
			"type": "individual",
			"url": "https://paypal.me/NatoBoram/5"
		}
	],
	"repository": "github:NatoBoram/heroicons.svelte",
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && vite build",
		"build:watch": "svelte-kit sync && vite build -w",
		"package": "svelte-kit sync && svelte-package",
		"package:watch": "svelte-kit sync && svelte-package -w",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "svelte-kit sync && storybook build",
		"build-heroicons": "tsx ./scripts/build_heroicons.ts"
	},
	"devDependencies": {
		"@playwright/test": "^1.29.2",
		"@storybook/addon-docs": "7.0.0-beta.20",
		"@storybook/addon-essentials": "7.0.0-beta.20",
		"@storybook/addon-interactions": "7.0.0-beta.20",
		"@storybook/addon-links": "7.0.0-beta.20",
		"@storybook/blocks": "7.0.0-beta.20",
		"@storybook/svelte": "7.0.0-beta.20",
		"@storybook/sveltekit": "7.0.0-beta.20",
		"@storybook/testing-library": "^0.0.13",
		"@storybook/types": "7.0.0-alpha.44",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.7",
		"@sveltejs/package": "^1.0.2",
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.48.0",
		"@typescript-eslint/parser": "^5.48.0",
		"autoprefixer": "^10.4.13",
		"eslint": "^8.31.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-storybook": "^0.6.8",
		"eslint-plugin-svelte3": "^4.0.0",
		"heroicons": "^2.0.13",
		"postcss": "^8.4.21",
		"prettier": "^2.8.2",
		"prettier-plugin-svelte": "^2.9.0",
		"prettier-plugin-tailwindcss": "^0.2.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "7.0.0-beta.20",
		"svelte": "^3.55.0",
		"svelte-check": "^2.10.3",
		"tailwindcss": "^3.2.4",
		"tslib": "^2.4.1",
		"tsx": "^3.12.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.4",
		"vitest": "^0.25.8"
	},
	"publishConfig": {
		"registry": "https://npm.pkg.github.com"
	},
	"type": "module"
}
